timeseries
  used_bytes          = sum(isilon.storagepool.bytes.used),
  used_hdd_bytes      = sum(isilon.storagepool.bytes.hdd.used),
  used_sdd_bytes      = sum(isilon.storagepool.bytes.sdd.used),
  total_bytes         = sum(isilon.storagepool.bytes.total),
  total_hdd_bytes     = sum(isilon.storagepool.bytes.hdd.total),
  total_sdd_bytes     = sum(isilon.storagepool.bytes.sdd.total),
  by:{ clusterEntity = `dt.entity.ef2:com_dynatrace_extension_isilon_cluster` }
| fieldsAdd
  used_bytes          = arrayLast(used_bytes),
  used_hdd_bytes      = arrayLast(used_hdd_bytes),
  used_sdd_bytes      = arrayLast(used_sdd_bytes),
  total_bytes         = arrayLast(total_bytes),
  total_hdd_bytes     = arrayLast(total_hdd_bytes),
  total_sdd_bytes     = arrayLast(total_sdd_bytes)
| fieldsAdd
  used_GB             = used_bytes      / 1000000000.0,
  used_TB             = used_bytes      / 1000000000000.0,
  used_hdd_GB         = used_hdd_bytes  / 1000000000.0,
  used_hdd_TB         = used_hdd_bytes  / 1000000000000.0,
  used_sdd_GB         = used_sdd_bytes  / 1000000000.0,
  used_sdd_TB         = used_sdd_bytes  / 1000000000000.0,
  total_GB            = total_bytes     / 1000000000.0,
  total_TB            = total_bytes     / 1000000000000.0,
  total_hdd_GB        = total_hdd_bytes / 1000000000.0,
  total_hdd_TB        = total_hdd_bytes / 1000000000000.0,
  total_sdd_GB        = total_sdd_bytes / 1000000000.0,
  total_sdd_TB        = total_sdd_bytes / 1000000000000.0,
  used_GiB            = used_bytes      / 1073741824.0,
  used_TiB            = used_bytes      / 1099511627776.0,
  used_hdd_GiB        = used_hdd_bytes  / 1073741824.0,
  used_hdd_TiB        = used_hdd_bytes  / 1099511627776.0,
  used_sdd_GiB        = used_sdd_bytes  / 1073741824.0,
  used_sdd_TiB        = used_sdd_bytes  / 1099511627776.0,
  total_GiB           = total_bytes     / 1073741824.0,
  total_TiB           = total_bytes     / 1099511627776.0,
  total_hdd_GiB       = total_hdd_bytes / 1073741824.0,
  total_hdd_TiB       = total_hdd_bytes / 1099511627776.0,
  total_sdd_GiB       = total_sdd_bytes / 1073741824.0,
  total_sdd_TiB       = total_sdd_bytes / 1099511627776.0
| sort clusterEntity
