$json.alertsList |
ForEach-Object {
    if ($_.alertCode -eq 'CE3601105') {

        # pick the IP field you actually have (start with the obvious one)
        $ip = $_.ipAddress
        if (-not $ip -and $_.details) { $ip = $_.details.ipAddress }
        if (-not $ip -and $_.details) { $ip = $_.details.ip }

        [pscustomobject]@{
            AlertCode = $_.alertCode
            IP        = $ip
            Details   = $_.details
        }
    }
}


$json.alertsList |
Where-Object { $_.alertCode -eq 'CE3601105' } |
ForEach-Object {
    [pscustomobject]@{
        AlertCode = $_.alertCode
        IP        = $_.ipAddress   # change if your JSON uses a different property
        Details   = $_.details
    }
}
