$json.alertsList |
Where-Object { $_.alertCode -eq 'CE3601105' } |
ForEach-Object {

    $prop = $_.propertyList | Where-Object { $_.key -eq 'node_ip' } | Select-Object -First 1
    $ip   = $prop.value
    if (-not $ip) { $ip = $prop.values }   # some payloads use "values"

    if ($ip) {
        [pscustomobject]@{
            AlertCode   = $_.alertCode
            IP          = $ip
            ClusterName = $_.clusterName
            Severity    = $_.severity
            Id          = $_.id
        }
    }
}
