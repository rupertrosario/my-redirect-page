<dashboard>
  <label>Multi-Base Search Example</label>

  <!-- Base Search 1 -->
  <search id="base1">
    <query>index=security sourcetype=auth action=fail</query>
  </search>

  <!-- Base Search 2 -->
  <search id="base2">
    <query>index=security sourcetype=auth action=success</query>
  </search>

  <panel>
    <title>Failed Logins</title>
    <search base="base1">
      <query>| stats count by user</query>
    </search>
  </panel>

  <panel>
    <title>Successful Logins</title>
    <search base="base2">
      <query>| stats count by user</query>
    </search>
  </panel>
</dashboard>


| rex "detail='(?<detail_content>.*?)';results="

... | rex "detail='(?<detail_content>.*?)';results=" 
    | table detail_content


... | rex field=_raw "(?is)detail=.*?(?<detail_content>.*?)\s*;results=" 
    | table detail_content


| rex field=_raw "(?is)detail\s*=\s*(?P<q>['\"])(?<detail_content>.*?)(?=;\s*results\s*=)"


... | rex field=_raw "(?is)detail\s*=\s*(?P<q>['\"])(?<detail_content>.*?)(?=;\s*results\s*=)"
    | table detail_content


... | rex max_match=0 field=_raw "(?is)detail\s*=\s*(?P<q>['\"])(?<detail_content>.*?)(?=;\s*results\s*=)"
    | mvexpand detail_content
    | table detail_content

| rex field=_raw "(?is)detail\s*=\s*(?P<q>['\"])(?<detail_content>.*?);?\s*results\s*="


| rex field=_raw "(?is)detail=.*?(?<detail_content>.*?)\s*;results="



... | rex field=_raw "(?is)detail=.*?(?<detail_content>.*?)\s*;results=" 
    | table detail_content
