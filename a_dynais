timeseries total_bytes = avg(isilon.storagepool.bytes.total),
           by: { dt.entity.isilon:storagepool },
           from: -5m,
           interval: 1m
| fieldsAdd last_total_bytes = arrayLast(total_bytes)
| fieldsAdd total_TiB = last_total_bytes / 1099511627776.0
| fields entityName(dt.entity.isilon:storagepool),
         total_TiB
